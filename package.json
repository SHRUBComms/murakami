{
  "name": "murakami",
  "version": "2.0.0",
  "description": "SHRUB Coop's membership system",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/honeypieio/murakami"
  },
  "contributors": [
    "Ross Hudson <hello@rosshudson.co.uk> (https://rosshudson.co.uk)"
  ],
  "scripts": {
    "nodemon": "nodemon server.js",
    "start": "node server.js",
    "db-create": "npx sequelize-cli db:create",
    "db-destroy": "npx sequelize-cli db:drop",
    "db-migrate": "npx sequelize-cli db:migrate",
    "db-downgrade": "npx sequelize-cli db:migrate:undo",
    "tests": "mocha --recursive || true",
    "tests --verbose": "mocha --recursive",
    "lint": "eslint \"**/*.{js,json}\" --ignore-pattern \"**/*.min.js\"",
    "format": "prettier --write \"**/*.{js,json}\""
  },
  "lint-staged": {
    "**/*.{js,json}": [
      "prettier --check"
    ],
    "*.{json,md}": "prettier --check"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "GPL-3.0",
  "dependencies": {
    "@handlebars/allow-prototype-access": "^1.0.3",
    "async": "^2.6.3",
    "bcrypt": "^5.0.1",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "clean-html": "^1.5.0",
    "connect-flash": "*",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.3.3",
    "cors": "^2.8.5",
    "cron": "^1.7.2",
    "dotenv": "^6.2.0",
    "email-validator": "^2.0.4",
    "export-to-csv": "^0.2.1",
    "express": "^4.21.1",
    "express-back": "*",
    "express-handlebars": "^3.1.0",
    "express-recaptcha": "^4.1.1",
    "html2plaintext": "^2.1.4",
    "lodash": "^4.17.21",
    "mailchimp-api-v3": "^1.13.1",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "mysql": "^2.17.1",
    "mysql2": "^1.7.0",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.7.2",
    "nodemailer-html-to-text": "^3.2.0",
    "nodemon": "^3.0.2",
    "passport": "^0.5.3",
    "passport-local": "*",
    "passport-oauth2": "^1.7.0",
    "pusher": "^3.0.0",
    "request": "^2.88.0",
    "sanitize-html": "^2.6.1",
    "sequelize": "^5.22.5",
    "sonarlint": "^1.1.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin-js": "^2.7.2",
    "chai": "^4.2.0",
    "credit-card-validate": "^0.9.3",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.10",
    "mocha": "^9.1.3",
    "prettier": "^3.3.3"
  }
}
